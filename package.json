{
  "name": "mailbots",
  "version": "0.1.1-beta",
  "description": "Create email-based bots with MailBots.com",
  "main": "mailbots.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha test -w --full-trace",
    "test:inspect": "NODE_ENV=test mocha test --full-trace --inspect-brk -w",
    "docs:build": "doctoc . --maxlevel 2",
    "dev:docs": "documentation -c documentation.yml serve",
    "dev:docs:build": "documentation -c documentation.yml -f html build -o docs-html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailbots/mailbots"
  },
  "author": "Reilly Sweetland <esweetland@gmail.com> (https://reillysweetland.com/)",
  "license": "(c) 2018 (unpublished).",
  "devDependencies": {
    "chai": "4.1.2",
    "doctoc": "1.3.1",
    "mocha": "5.2.0",
    "supertest": "3.1.0"
  },
  "dependencies": {
    "@mailbots/mailbots-sdk": "0.1.1-beta",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "lodash": "4.17.10",
    "request": "2.87.0",
    "url-join": "4.0.0",
    "uuid": "3.3.2"
  }
}
